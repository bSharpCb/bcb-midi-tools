                <option value="3">Octave up</option>
                <option value="4">Octave down</option>
                <option value="5">Third up</option>
                <option value="6">Third down</option>
                </select>
            </div>
        
        <button id="synbtn" onclick="makeSynth()">New synth</button>
        
        <button id="render" onclick="abcRender()">Render notes</button>
        <button id="abcToggle" onclick="abcToggle()">Edit mode toggle</button>
        <div id="editReadme">
            MIDI notes : corresponding edit function
            21 : edit left hand
            23 : edit right hand 
            27 28 29 : 2 4 8(time)
            

        </div>
        <p id='rightHeader'>right hand</p>
        <textarea id="right">[V: PianoRightHand]|</textarea>
        <p id='leftHeader'>left hand</p>
        <textarea id="left">[V: PianoLeftHand]|</textarea>
        <p>header</p>
        <textarea id="header">X:1
T:Song title 
L:1/4
%%staves {(PianoRightHand) (PianoLeftHand)}
V:PianoRightHand clef=treble
V:PianoLeftHand clef=bass
K:Ab
</textarea>

      
        
