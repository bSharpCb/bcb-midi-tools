        <div id="content">

        
            <div id="osc1">
            Oscillator 1
                <select id="osc1-type">
                <option value="0">Sine</option>
                <option value="1">Square</option>
                <option value="2">Sawtooth</option>
                <option value="3">Triangle</option>
                </select>
    
    
                <select id="osc1-harmonize">
                <option value="0">None</option>
                <option value="1">5th up</option>
                <option value='2'>5th down</option>
                <option value="3">Octave up</option>
                <option value="4">Octave down</option>
                <option value="5">Third up</option>
                <option value="6">Third down</option>
                </select>
            </div>
            <div id="osc2">
            Oscillator 2
                <select id="osc2-type">
                <option value="0">Sine</option>
                <option value="1">Square</option>
